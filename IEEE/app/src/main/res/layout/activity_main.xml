        android:background="#ffffcc"

        />

    <Button
        android:text="9"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/button24"
        android:layout_alignBaseline="@+id/button23"
        android:layout_alignBottom="@+id/button23"
        android:layout_alignLeft="@+id/button20"
        android:layout_alignStart="@+id/button20"
        android:onClick="onClick"
        android:background="#ffffcc"

        />

    <Button
        android:text="0"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/button26"
        android:layout_alignRight="@+id/button23"
        android:layout_alignEnd="@+id/button23"
        android:id="@+id/button27"
        android:background="#ffffcc"

        />

    <Button
        android:text="."
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:id="@+id/button26"
        android:layout_below="@+id/button22"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="onClick"
        android:background="#ffffcc"

        />

    <Button
        android:text="-"
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/button25"
        android:layout_above="@+id/button26"
        android:layout_alignLeft="@+id/button21"
        android:layout_alignStart="@+id/button21"
        android:onClick="onClickSigns"
        android:background="#cc99cc"

        />

    <Button
        android:text="+"
        android:layout_width="75dp"
        android:layout_height="wrap_content"
        android:id="@+id/button29"
        android:layout_alignBottom="@+id/button27"
        android:layout_alignLeft="@+id/button25"
        android:layout_alignStart="@+id/button25"
